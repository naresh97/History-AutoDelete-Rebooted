{
  "background": {
    "scripts": [
  "global_files/browserDetect.js",
	"global_files/browser-polyfill.js",
	"global_files/redux.js",
	"global_files/redux-thunk.js",
	"global_files/redux-webext.js",
	"global_files/shortid.js",
	"backgroundBundle.js"
	]
  },
  "description": "Control your history. Automatically and selectively delete history.",
  "homepage_url": "https://github.com/naresh97/History-AutoDelete-Rebooted",
  "browser_action": {
    "default_icon": {
      "24": "icons/icon_24.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png"
    },
    "default_title": "History AutoDelete Rebooted",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "options_ui": {
  	"page": "settings/settings.html",
  	"open_in_tab" : true
  },
  "permissions": [
    "activeTab",
    "history",
    "tabs",
    "storage",
    "alarms"
  ],
  "manifest_version": 2,
  "name": "History AutoDelete Rebooted",
  "version": "3.0.0"
}
